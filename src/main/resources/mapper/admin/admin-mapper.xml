<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.oee.admin.model.dao.AdminDao">

	<select id="selectAdminMemberList" resultMap="adminMemberMap">
		select
			*
		from
			member
		where
			delete_date is null
		order by
			enroll_date desc
	</select>
	
	<resultMap type="member" id="adminMemberMap"></resultMap>
	
	<select id="selectAdminBoardReport" resultMap="adminBoardReportMap">
		select
			*
		from
			board_report
		order by
			reg_date desc
	</select>
	
	<resultMap type="boardReport" id="adminBoardReportMap"></resultMap>
	
	<select id="selectAdminUserReport" resultMap="adminUserReportMap">
		select
			*
		from
			user_report
		order by
			reg_date desc
	</select>
	
	<resultMap type="userReport" id="adminUserReportMap"></resultMap>

</mapper>